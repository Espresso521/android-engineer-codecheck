<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">
    
    <application
        android:allowBackup="true"
        android:name="jp.co.yumemi.android.code_check.SearchApplication"
        android:icon="@mipmap/github_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/github_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidEngineerCodeCheck"
        android:fullBackupContent="@xml/backup_descriptor">
        <activity
            android:name="jp.co.yumemi.android.code_check.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="jp.co.yumemi.android.code_check.MainActivity"
            android:exported="false">
        </activity>

        <activity
            android:name="jp.co.yumemi.android.code_check.TestActivity"
            android:exported="false">
        </activity>
    </application>

</manifest>